    "version": "0.2.0",
    "configurations": [
        {
            "name": "order_resto",
            "request": "launch",
            "type": "dart",
            "args": [
             "-d", "chrome", 
             "--web-renderer", "html"
            ]
        },
        {
            "name": "order_resto_web_server",
            "request": "launch",
            "type": "dart",
            "args": [
             "-d", "web-server", 
             "--web-port", "8080",
             "--web-hostname", "0.0.0.0"
            ]
        },
        {
            "name": "order_resto (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "order_resto (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}